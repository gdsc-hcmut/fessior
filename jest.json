{
    "moduleFileExtensions": [
          "ts",
          "tsx",
          "js",
          "json"
      ],
      "transform": {
          "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "/src/.*\\.(test|spec).(ts|tsx|js)$",
      "collectCoverageFrom" : ["src/**/*.{js,jsx,tsx,ts}", "!**/node_modules/**", "!**/vendor/**"],
      "coverageReporters": ["json", "lcov"],
      "coverageThreshold": {
        "global": {
          "branches": 80,
          "functions": 80,
          "lines": 80,
          "statements": 80
        }
      }
  }
